    <meta name="viewport" content="width=device-width, initial-scale=0">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <title>Bug Tracker App</title>
    <meta name="description" content="Charisma, a fully featured, responsive, HTML5, Bootstrap admin template.">
    <meta name="author" content="Muhammad Usman">

    <!-- The styles -->
    <link id="bs-css" href="~/Templates Contents/css/bootstrap-cerulean.min.css" rel="stylesheet">
    <link href="~/Templates Contents/css/charisma-app.css" rel="stylesheet" />
    <link href="~/Templates Contents/bower_components/fullcalendar/dist/fullcalendar.css" rel="stylesheet" />
    <link href='~/Templates Contents/fullcalendar/dist/fullcalendar.print.css' rel='stylesheet' media='print'>
    <link href='~/Templates Contents/chosen/chosen.min.css' rel='stylesheet'>
    <link href='~/Templates Contents/colorbox/example3/colorbox.css' rel='stylesheet'>
    <link href='~/Templates Contents/responsive-tables/responsive-tables.css' rel='stylesheet'>
    <link href='~/Templates Contents/bootstrap-tour/build/css/bootstrap-tour.min.css' rel='stylesheet'>
    <link href='~/Templates Contents/css/jquery.noty.css' rel='stylesheet'>
    <link href='~/Templates Contents/css/noty_theme_default.css' rel='stylesheet'>
    <link href='~/Templates Contents/css/elfinder.min.css' rel='stylesheet'>
    <link href='~/Templates Contents/css/elfinder.theme.css' rel='stylesheet'>
    <link href='~/Templates Contents/css/jquery.iphone.toggle.css' rel='stylesheet'>
    <link href='~/Templates Contents/css/uploadify.css' rel='stylesheet'>
    <link href='~/Templates Contents/css/animate.min.css' rel='stylesheet'>

    <!-- jQuery -->
    <script src="~/Templates Contents/bower_components/jquery/jquery.min.js"></script>

    <!-- The HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <!-- The fav icon -->
    <link rel="shortcut icon" href="~/Templates Contents/bower_components/datatables/media/images/favicon.ico">

</head>

<body>
    <!-- topbar starts -->
    <div class="navbar navbar-default" role="navigation">

        <div class="navbar-inner">
            <a class="navbar-brand" href="~/Views/Home/Index.cshtml">
                <img alt="Charisma Logo" src="~/Pictures/Logo.png" class="hidden-xs" />
                <span> Bug </span>
            </a>

            <!-- user dropdown starts -->
            <div class="btn-group pull-right">

                @Html.Partial("_LoginPartial")

            </div>

        </div>
    </div>
    <!-- topbar ends -->
    <div class="ch-container">
        <div class="row">

            <!-- left menu starts -->
            <div class="col-sm-2 col-lg-2">
                <div class="sidebar-nav">
                    <div class="nav-canvas">
                        <div class="nav-sm nav nav-stacked">

                        </div>
                        <ul class="nav nav-pills nav-stacked main-menu">
                            <li class="nav-header">Main</li>
                            <li> @Html.ActionLink("Dashboard", "Index", "Tickets") </li>
                            <li> @Html.ActionLink("Users", "Index", "ApplicationUsers")</li>
                            <li> @Html.ActionLink("Project", "index", "projects")</li>
                            <li> @Html.ActionLink("Profile", "About", "Home")</li>
                            <li> @Html.ActionLink("Contact", "Contact", "Home")</li>
                        </ul>
                    </div>
                </div>
            </div>
            <!--/span-->
            <!-- left menu ends -->

            <div id="content" class="col-lg-10 col-sm-10">
                <!-- content starts -->
                <div>
                    @RenderBody()
                </div>
            </div>
        </div>


        <hr>

        @*<footer class="row">
                <p class="col-md-9 col-sm-9 col-xs-12 copyright">
                    &copy; <a href="http://usman.it" target="_blank">
                        Muhammad
                        Usman
                    </a> 2012 - 2015
                </p>

                <p class="col-md-3 col-sm-3 col-xs-12 powered-by">
                    Powered by: <a href="http://usman.it/free-responsive-admin-template">Charisma</a>
                </p>
            </footer>*@

    </div><!--/.fluid-container-->
    <!-- external javascript -->
    @RenderSection("scripts", required: false)

    <script src="~/Templates Contents/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>

    <!-- library for cookie management -->
    <script src="~/Templates Contents/js/jquery.cookie.js"></script>
    <!-- calender plugin -->
    <script src='~/Templates Contents/bower_components/moment/min/moment.min.js'></script>
    <script src='~/Templates Contents/bower_components/fullcalendar/dist/fullcalendar.min.js'></script>
    <!-- data table plugin -->
    <script src='~/Templates Contents/js/jquery.dataTables.min.js'></script>

    <!-- select or dropdown enhancer -->
    <script src="~/Templates Contents/bower_components/chosen/chosen.jquery.min.js"></script>
    <!-- plugin for gallery image view -->
    <script src="~/Templates Contents/bower_components/colorbox/jquery.colorbox-min.js"></script>
    <!-- notification plugin -->
    <script src="~/Templates Contents/js/jquery.noty.js"></script>
    <!-- library for making tables responsive -->
    <script src="~/Templates Contents/bower_components/responsive-tables/responsive-tables.js"></script>
    <!-- tour plugin -->
    <script src="~/Templates Contents/bower_components/bootstrap-tour/build/js/bootstrap-tour.min.js"></script>
    <!-- star rating plugin -->
    <script src="~/Templates Contents/js/jquery.raty.min.js"></script>
    <!-- for iOS style toggle switch -->
    <script src="~/Templates Contents/js/jquery.iphone.toggle.js"></script>
    <!-- autogrowing textarea plugin -->
    <script src="~/Templates Contents/js/jquery.autogrow-textarea.js"></script>
    <!-- multiple file upload plugin -->
    <script src="~/Templates Contents/js/jquery.uploadify-3.1.min.js"></script>
    <!-- history.js for cross-browser state change on ajax -->
    <script src="~/Templates Contents/js/jquery.history.js"></script>
    <!-- application script for Charisma demo -->
    <script src="~/Templates Contents/js/charisma.js"></script>


</body>
</html>

